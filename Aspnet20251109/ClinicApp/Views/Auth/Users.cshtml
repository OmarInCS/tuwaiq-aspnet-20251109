@model List<AppUser>


<h5 class="card-title">
    Users
</h5>
<ul>
    @foreach (var user in Model)
    {
        <li>

            @if (user.ProfilePicture != null)
            {
                <img src="data:image;base64,@Convert.ToBase64String(user.ProfilePicture)" alt="Profile Picture" width="50" height="50" />
            }
            @user.Email
        </li>
    }
</ul>
    
